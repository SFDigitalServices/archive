{
  "eslint.lintTask.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.tabSize": 2,
  "yaml.format.enable": true,
  "yaml.customTags": [
    "!!import"
  ],
  "[yaml]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    },
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    }
  }
}